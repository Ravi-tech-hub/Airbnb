<%if(error.length>0) {%>
  <div class="bg-red-100 border border-red-100 text-red-500 px-4 py-3 rounded relative" role="alert">
    <strong class="font-bold">Error!</strong>
    <span class="block sm:inline">
      <%error.forEach(error=>
        {
        %>
        <li>
          <%=error%>
        </li>
        <%})%>
    </span>
  </div>
  <%}%>